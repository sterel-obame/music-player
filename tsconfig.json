{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "Config/*": ["src/Config/*"],
      "Constants/*": [".rc/Constants/*"],
      "Assets/*": ["assets/*"],
      "Navigation/*": ["src/Navigation/*"],
      "Components/*": ["./src/Components/*"],
      "Screens/*": ["src/Screens/*"],
      "context/*": ["src/context/*"],
      "hooks/*": ["src/hooks/*"],
      "utils/*": ["src/utils/*"],
      "services/*": ["src/services/*"],
    }
  }
}
